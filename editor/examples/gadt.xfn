data Foo : Type {
    MkFoo : Foo
}

data Bar : Type {
    MkBar : Bar
}

data GADT(a: Type) : Type {
    WrapFoo(x: Foo) : GADT(Foo),
    WrapBar(x: Bar) : GADT(Bar),
}

impl GADT {
    def GADT(a).unwrap(a: Type) : a {
        WrapFoo(x: Foo){h: a = Foo} => x,
        WrapBar(x: Bar){h: a = Bar} => x,
    }
}
